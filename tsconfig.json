{
  "compilerOptions": {
    "target": "es5",
    "jsx": "react-jsx",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "rootDir": "./",
    "baseUrl": "./",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
  },
  "include": [
    // "**/*.ts",
    // "**/*.tsx",
    // "**/*/.tsx",
    // "**/*.js",
    // "**/*/.js",
    // "**/*/.jsx",
    // "**/*/.css",
    // "src/index.tsx",
    "src"
  ],
  "exclude": [
    "docs",
    ".vscode",
    "build"
  ]
}